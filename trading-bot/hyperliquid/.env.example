# Hyperliquid Trading Bot - Environment Variables
# Copy this to .env and fill in your values

# === CORE HYPERLIQUID ===
HL_AGENT_KEY=your_private_agent_key_here
HL_MAIN_ADDR=your_public_wallet_address_here

# === RPC NODES ===
ALCHEMY_HYPER_URL=https://hyperliquid-mainnet.g.alchemy.com/v2/your_key
HYPER_PUBLIC_RPC=https://rpc.hyperliquid.xyz/evm

# === SCOUTING & ANALYTICS ===
NANSEN_API_KEY=your_nansen_key_here
DWELLIR_API_KEY=your_dwellir_key_here
DUNE_API_KEY=your_dune_key_here

# === STRATEGY CONFIG ===
SYMBOL=BTC
LEVERAGE_SHORT=15
LEVERAGE_LONG=3
GRID_LEVELS=5
GRID_RANGE_PCT=0.05
