# Molt Cops Badge Deployment Configuration
# Copy to .env and fill in values

# Deployer wallet private key (without 0x prefix is fine)
DEPLOYER_PRIVATE_KEY=

# Base RPC URLs (optional - defaults provided)
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Basescan API key for contract verification
# Get from: https://basescan.org/myapikey
BASESCAN_API_KEY=

# Backend minter address (optional)
# This address will be authorized to call minterMint()
# Leave empty to configure later via setMinter()
MINTER_ADDRESS=

# Gas reporting (optional)
REPORT_GAS=false
